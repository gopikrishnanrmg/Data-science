setwd("~/R project/World happiness report")
df17<-read.csv("2017.csv", stringsAsFactors = TRUE)
df16<-read.csv("2016.csv", stringsAsFactors = TRUE)
df15<-read.csv("2015.csv", stringsAsFactors = TRUE)
sum(is.na(df17))
sum(is.na(df16))
sum(is.na(df15))
lm1<-lm(Happiness.Score~Lower.Confidence.Interval+Upper.Confidence.Interval+Economy..GDP.per.Capita.+Family+Health..Life.Expectancy.+Freedom+Trust..Government.Corruption.+Generosity,data = df16)
lm1
result1<-predict(lm1,df15)
result1
plot(df15$Happiness.Score,result1)
plot(df15$Happiness.Score,result1)
plot(df15$Happiness.Score,result1)
result1<-predict(lm1,df15)
result1<-predict(lm1,df15)
